  		<wx:footer top="1"/>	
  	]]>
 </resource>
 
 <resource key="dataexport.template.default" type="com.wabacus.config.resource.TemplateRes"><!-- 下面next和last的写法主要是为了说明可以自己提供翻页按钮或label -->
