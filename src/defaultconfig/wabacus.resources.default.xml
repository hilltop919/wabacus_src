  <!-- 定义了wabacus按钮样式 -->
  <resource key="wabacus.button.default" type="com.wabacus.config.resource.ButtonRes">
  	 <button styleproperty="class='cls-button' onmouseover=&quot;this.className='cls-button-mouseover'&quot; onmouseout=&quot;this.className='cls-button'&quot;" 
  				disabledstyleproperty="class='cls-button-disabled'"/>
  </resource>
  <resource key="wabacus.button2.default" type="com.wabacus.config.resource.ButtonRes">
  	 <button styleproperty="class='cls-button2' onmouseover=&quot;this.className='cls-button2-mouseover'&quot; onmouseout=&quot;this.className='cls-button2'&quot;"  
  			  disabledstyleproperty="class='cls-button2-disabled'"/>
  </resource>
  
 <!-- 默认"搜索"按钮 --><!-- 这里positionorder配置为－1，因为在搜索栏中，它必须显示在所有其它按钮的前面 -->
 <resource key="search.button.default" type="com.wabacus.config.resource.ButtonRes">
  	<button  position="searchbox" positionorder="-1" label="${search.button.default.label}"  
  				extends="${wabacus.button.default}" class="com.wabacus.system.buttons.SearchButton"/>
 </resource>
  <!-- 下面的按钮当没有指定position属性时，将不会做为按钮的形式显示出来；当没有配置menulabel属性时，将不会显示在右键菜单项中 -->
  <!-- 如果要在position中指定它显示在多个位置，则用|号分隔，目前只支持指定title和searchbox，如果要在header、footer、navigate位置显示按钮，则需要在它们的模板中用<wx:button/>进行显示 -->
 <!-- 默认"新增"按钮 -->
 <resource key="add.button.default" type="com.wabacus.config.resource.ButtonRes">
  	<button position="title|contextmenu" positionorder="1" label="${add.button.default.label}" menulabel="${add.menu.default.label}" menugroup="1110"
  			  extends="${wabacus.button.default}" class="com.wabacus.system.buttons.AddButton"/>
 </resource>
 <!-- 默认"修改"按钮 -->
 <resource key="modify.button.default" type="com.wabacus.config.resource.ButtonRes">
