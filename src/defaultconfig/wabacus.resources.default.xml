  <resource key="searchbox_outer_style">
  	<!-- 搜索框样式 %WEBROOT%为根URL占位符，%SKIN%为当前应用配置的主题风格占位符；%SEARCHBOX_CONTENT%为搜索栏内容的占位符（包括输入框，搜索按钮，配置为显示在搜索栏上的按钮等），均在运行时由框架自动构造-->
     <![CDATA[
	  <table class="cls-search-table" cellpadding="0" cellspacing="0" width="100%" ><tbody>
	    <tr><td width="6" align="left" valign="top" background="%WEBROOT%webresources/skin/%SKIN%/images/search05.gif">
	     <img src="%WEBROOT%webresources/skin/%SKIN%/images/search01.gif" height="6" width="6"/></td>
	     <td rowspan="3" style="BORDER-TOP: #cfcfcf 1px solid; LINE-HEIGHT: 23px; BORDER-BOTTOM: #cfcfcf 1px solid">%SEARCHBOX_CONTENT%</td>
		 <td width="6" align="right" valign="top" background="%WEBROOT%webresources/skin/%SKIN%/images/search06.gif"><img src="%WEBROOT%webresources/skin/%SKIN%/images/search03.gif" height="6" width="6"/></td></tr>
		 <tr><td background="%WEBROOT%webresources/skin/%SKIN%/images/search05.gif"></td><td background="%WEBROOT%webresources/skin/%SKIN%/images/search06.gif"></td></tr>
		 <tr><td width="6" align="left" valign="bottom" background="%WEBROOT%webresources/skin/%SKIN%/images/search05.gif"><img src="%WEBROOT%webresources/skin/%SKIN%/images/search02.gif" height="6" width="6"/></td><td width="6" align="right" valign="bottom" background="%WEBROOT%webresources/skin/%SKIN%/images/search06.gif"><img src="%WEBROOT%webresources/skin/%SKIN%/images/search04.gif" height="6" width="6"/></td></tr>
		</tbody></table>
     ]]>
  </resource>
